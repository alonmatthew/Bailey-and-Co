<style media="screen">
  .search-results{
    padding: 2%;
  }
</style>
<div class="col-lg-8 well border-radius">
  <h1 class="text-center">Search Results</h1>
      <% businesses.forEach((b) => { %>
        <div class="search-results">
          <a href="/location/<%= b.id %>">
            <img src="<%= b.image_url %>" alt="">
            <%= b.name %> (<%= b.location.city %>)
          </a>
        </div>

      <% }) %>
</div>
<div class="col-lg-4 well border-radius">
  <h1 class="text-center">Map it!</h1>
</div>
